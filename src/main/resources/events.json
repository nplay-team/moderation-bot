  "moderationCreateEvent": {
    "title": "{ $type } | #{ $id }",
    "fields": [
      {
        "name": "Typ",
        "value": "{ $type }"
      },
      {
        "name": "Betroffener",
        "value": "{ $target }"
      },
      {
        "name": "Moderator",
