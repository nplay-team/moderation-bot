{
  "moderationCreateEvent": {
    "title": "{ $type } | #{ $id }",
    "fields": [
      {
        "name": "Typ",
        "value": "{ $type }"
      },
      {
        "name": "Betroffener",
        "value": "<@{ $targetId }> ({ $targetUsername })"
      },
      {
        "name": "Moderator",
        "value": "<@{ $issuerId }> ({ $issuerUsername })"
      },
      {
        "name": "Begründung",
        "value": "{ $reason }"
      },
      {
        "name": "Datum",
        "value": "<t:{ $createdAt }:F>"
      },
      {
        "name": "Aktiv bis",
        "value": "{ $until }"
      }
    ],
    "color": "{ $colorDefault }"
  },
  "moderationRevertEvent": {
    "title": "REVIDIERUNG | { $type } | #{ $id }",
    "fields": [
      {
        "name": "Typ",
        "value": "{ $type }"
      },
      {
        "name": "Betroffener",
        "value": "<@{ $targetId }> ({ $targetUsername })"
      },
      {
        "name": "Moderator",
        "value": "<@{ $issuerId }> ({ $issuerUsername })"
      },
      {
        "name": "Revidierender Moderator",
        "value": "<@{ $revertedById }> ({ $revertedByUsername })"
      },
      {
        "name": "Revidierungsgrund",
        "value": "{ $revertingReason }"
      },
      {
        "name": "Datum",
        "value": "<t:{ $revertedAt }:F>"
      }
    ],
    "color": "{ $colorWarning }"
  },
  "moderationDeleteEvent": {
    "title": "LÖSCHUNG | { $type } | #{ $id }",
    "fields": [
      {
        "name": "Typ",
        "value": "{ $type }"
      },
      {
        "name": "Betroffener",
        "value": "<@{ $targetId }> ({ $targetUsername })"
      },
      {
        "name": "Moderator",
        "value": "<@{ $issuerId }> ({ $issuerUsername })"
      },
      {
        "name": "Löschender Moderator",
        "value": "<@{ $deletedById }> ({ $deletedByUsername })"
      },
      {
        "name": "Datum",
        "value": "<t:{ $createdAt }:F>"
      }
    ],
    "color": "{ $colorError }"
  },
  "bulkMessageDeleteEvent": {
    "title": "Massenlöschung von Nachrichten",
    "fields": [
      {
        "name": "Menge",
        "value": "{ $amount } Nachrichten"
      },
      {
        "name": "Moderator",
        "value": "{ $issuer }"
      },
      {
        "name": "Datum",
        "value": "<t:{ $createdAt }:F>"
      }
    ],
    "color": "{ $colorDefault }"
  },
  "spielersucheAusschlussEvent": {
    "title": "Spielersuche-Ausschluss",
    "fields": [
      {
        "name": "Betroffener",
        "value": "{ $target }"
      },
      {
        "name": "Moderator",
        "value": "{ $issuer }"
      },
      {
        "name": "Datum",
        "value": "<t:{ $createdAt }:F>"
      }
    ],
    "color": "{ $colorDefault }"
  },
  "spielersucheAusschlussRevertEvent": {
    "title": "Spielersuche-Ausschluss-Aufhebung",
    "fields": [
      {
        "name": "Betroffener",
        "value": "{ $target }"
      },
      {
        "name": "Moderator",
        "value": "{ $issuer }"
      },
      {
        "name": "Datum",
        "value": "<t:{ $createdAt }:F>"
      }
    ],
    "color": "{ $colorDefault }"
  }
}