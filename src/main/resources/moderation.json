{
  "moderationActExecuted": {
    "title": "{ $type } erfolgreich ausgeführt!",
    "color": "{ $colorSuccess }"
  },
  "moderationTargetBlocked": {
    "title": "Fehler",
    "description": "Der angegebene Nutzer { $target } wird bereits durch { $moderator } moderiert!",
    "color": "{ $colorError }"
  },
  "moderationActTargetInfo": {
    "title": "{ $title }",
    "description": "{ $description }",
    "color": "{ $color }",
    "fields": [
      {
        "name": "Begründung",
        "value": "{ $reason }",
        "inline": true
      },
      {
        "name": "Datum",
        "value": "{ $date }",
        "inline": true
      },
      {
        "name": "Aufhebung",
        "value": "{ $until }"
      },
      {
        "name": "Regel, gegen die du verstoßen hast",
        "value": "{ $paragraph }",
        "inline": false
      },
      {
        "name": "ID",
        "value": "#{ $id }"
      },
      {
        "name": "Referenznachricht",
        "value": "> { $referenceMessage }"
      }
    ],
    "footer": {
      "text": "Bei Fragen oder Problemen wende dich bitte an den Ticket-Support!"
    }
  },
  "moderationActReverted": {
    "title": "{ $type } aufgehoben",
    "description": "Dein { $type } auf dem **NPLAY** Discord Server wurde aufgehoben!",
    "fields": [
      {
        "name": "{ $type } vom",
        "value": "{ $date }",
        "inline": true
      },
      {
        "name": "ID",
        "value": "#{ $id }"
      },
      {
        "name": "Begründung",
        "value": "{ $reason }"
      },
      {
        "name": "Revidierender Moderator",
        "value": "{ $revertedBy }"
      }
    ],
    "color": "{ $colorSuccess }"
  },
  "reversionSuccessful": {
    "title": "Erfolgreich",
    "description": "Die Moderationshandlung mit der ID **#{ $id }** wurde erfolgreich rückgängig gemacht!",
    "color": "{ $colorSuccess }"
  },
  "deletionSuccessful": {
    "title": "Erfolgreich",
    "description": "Die Moderationshandlung mit der ID **#{ $id }** wurde erfolgreich gelöscht!",
    "color": "{ $colorSuccess }"
  },
  "modlogHeader": {
    "author": {
      "name": "{ $username }",
      "icon_url": "{ $avatarUrl }"
    },
    "title": "NPLAY-Moderation - Datenauskunft",
    "fields": [
      {
        "name": "Name",
        "value": "{ $name }",
        "inline": false
      },
      {
        "name": "Nutzer-ID",
        "value": "{ $userId }",
        "inline": false
      },
      {
        "name": "Avatar",
        "value": "[Link]({ $avatarUrl })",
        "inline": false
      },
      {
        "name": "Rollen",
        "value": "{ $roles }",
        "inline": false
      },
      {
        "name": "Erstellt am",
        "value": "{ $createdAt }",
        "inline": false
      },
      {
        "name": "Beigetreten am",
        "value": "{ $joinedAt }",
        "inline": false
      }
    ],
    "color": "{ $colorDefault }"
  },
  "modlogActs": {
    "title": "NPLAY-Moderation - Moderationshandlungen",
    "color": "{ $colorDefault }",
    "footer": {
      "text": "Seite { $page } von { $maxPage }"
    }
  },
  "moderationActDetail": {
    "title": "NPLAY-Moderation - Moderationshandlung",
    "color": "{ $colorDefault }",
    "fields": [
      {
        "name": "ID",
        "value": "#{ $id }"
      },
      {
        "name": "Typ",
        "value": "{ $type }"
      },
      {
        "name": "Datum",
        "value": "{ $created }"
      },
      {
        "name": "Moderator",
        "value": "{ $issuer }"
      },
      {
        "name": "Begründung",
        "value": "{ $reason }"
      }
    ]
  },
  "noteCreated": {
    "title": "Notiz erstellt",
    "description": "Die Notiz mit der ID **${ $id }** wurde erfolgreich erstellt!",
    "fields": [
      {
        "name": "Inhalt",
        "value": "{ $content }"
      },
      {
        "name": "Benutzer",
        "value": "{ $target }"
      },
      {
        "name": "Erstellt am",
        "value": "{ $createdAt }"
      },
      {
        "name": "Ersteller",
        "value": "{ $createdBy }"
      }
    ],
    "color": "{ $colorSuccess }"
  },
  "reversionFailed": {
    "title": "Fehler",
    "description": "Die Moderationshandlung mit der ID **#{id}** wurde bereits rückgängig gemacht!",
    "color": "{ $colorError }"
  }
}