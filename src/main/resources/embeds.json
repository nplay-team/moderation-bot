    "color": "{ $colorSuccess }"
  },
  "modlogHeader": {
    "author": {
      "name": "{ $username }",
      "iconUrl": "{ $avatarUrl }"
    },
    "title": "NPLAY-Moderation - Datenauskunft",
    "fields": [
      {
        "name": "Name",
        "value": "{ $effectiveName } <@{ $userId }>",
        "inline": false
      },
      {
        "name": "Nutzer-ID",
        "value": "{ $userId }",
        "inline": false
      },
      {
        "name": "Avatar",
        "value": "[Link]({ $avatarUrl })",
        "inline": false
      },
      {
        "name": "Rollen",
        "value": "{ $roles }",
        "inline": false
      },
      {
        "name": "Erstellt am",
        "value": "<t:{ $createdAt }:F> (<t:{ $createdAt }:R>)",
        "inline": false
      },
      {
        "name": "Beigetreten am",
        "value": "<t:{ $joinedAt }:F> (<t:{ $joinedAt }:R>)",
        "inline": false
      }
    ],
    "color": "{ $colorDefault }"
  },
  "modlogActs": {
    "title": "NPLAY-Moderation - Moderationshandlungen",
    "color": "{ $colorDefault }",
    "footer": {
      "text": "Seite { $page } von { $maxPage }"
    }
  },
  "moderationActDetail": {
    "title": "NPLAY-Moderation - Moderationshandlung",
    "color": "{ $colorDefault }",
    "fields": [
      {
        "name": "ID",
        "value": "#{ $id }"
      },
      {
        "name": "Typ",
        "value": "{ $type }"
      },
      {
        "name": "Datum",
        "value": "<t:{ $date }:F> (<t:{ $date }:R>)"
      },
      {
        "name": "Moderator",
        "value": "<@{ $issuerId }> ({ $issuerUsername })"
      },
      {
        "name": "Begründung",
        "value": "{ $reason }"
      },
      {
        "name": "Regel",
        "value": "{ $paragraph }"
      },
      {
        "name": "Dauer",
        "value": "{ $duration }"
      },
      {
        "name": "Referenznachricht",
        "value": "> { $referenceMessage }"
      },
      {
        "name": "Aufhebung",
        "value": "<t:{ $until }:F> (<t:{ $until }:R>)"
      },
      {
        "name": "Revidiert am",
        "value": "<t:{ $revertedAt }:F> (<t:{ $revertedAt }:R>)"
      },
      {
        "name": "Revidierender Moderator",
        "value": "<@{ $revertedById }> ({ $revertedByUsername })"
      },
      {
        "name": "Revidierungsgrund",
        "value": "{ $reversionReason }"
      }
    ]
  },
  "noteCreated": {
    "title": "Notiz erstellt",
    "description": "Die Notiz mit der ID **${ $id }** wurde erfolgreich erstellt!",
