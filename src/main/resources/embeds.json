      {
        "name": "Inhalt",
        "value": "{ $content }"
      },
      {
        "name": "Benutzer",
        "value": "<@{ $targetId }> ({ $targetUsername })"
      },
      {
        "name": "Erstellt am",
        "value": "<t:{ $createdAt }:F> (<t:{ $createdAt }:R>)"
      },
      {
        "name": "Ersteller",
        "value": "<@{ $createdById }> ({ $createdByUsername })"
