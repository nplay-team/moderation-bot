      {
        "name": "Warn vom",
        "value": "<t:{ $date }>",
        "inline": true
      },
      {
        "name": "ID",
        "value": "#{ $id }"
      },
      {
        "name": "Begründung",
        "value": "{ $reason }"
      },
      {
        "name": "Revidierender Moderator",
        "value": "<@{ $revertedById }> ({ $revertedByUsername })"
      }
    ],
    "color": "{ $colorSuccess }"
  },
  "timeoutReverted": {
    "title": "Timeout aufgehoben",
