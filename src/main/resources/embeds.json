      {
        "name": "Timeout vom",
        "value": "<t:{ $date }>",
        "inline": true
      },
      {
        "name": "ID",
        "value": "#{ $id }"
      },
      {
        "name": "Begründung",
        "value": "{ $reason }"
      },
      {
        "name": "Zuständiger Moderator",
        "value": "<@{ $revertedById }> ({ $revertedByUsername })"
      }
