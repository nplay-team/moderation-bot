{
  "insufficientPermissions": {
    "title": "Fehlende Berechtigungen",
    "description": "`{ $name }` benötigt bestimmte Berechtigungen, um ausgeführt zu werden",
    "color": "{ $colorError }",
    "fields": [
      {
        "name": "Berechtigungen:",
        "value": "`{ $permissions }`"
      }
    ]
  },
  "typeAdaptingFailed": {
    "title": "Falsche Eingabe",
    "color": "{ $colorWarning }",
    "fields": [
      {
        "name": "Details",
        "value": "{ $details }"
      }
    ]
  },
  "constraintFailed": {
    "title": "Parameter Validation fehlgeschlagen",
    "description": "```{ $message }```",
    "color": "{ $colorError }"
  },
  "cooldown": {
    "title": "Cooldown",
    "description": "Du kannst diesen Command nicht für weitere { $cooldown }! nutzen",
    "color": "{ $colorWarning }"
  },
  "executionFailed": {
    "title": "Command Ausführung fehlgeschlagen",
    "description": "Bei der Command Ausführung ist ein plötzlicher Fehler aufgetreten. Bitte leite die Fehlermeldung an <@395908417879015424> weiter.",
    "fields": [
      {
        "name": "Fehlermeldung",
        "value": "{ $colorError }"
      }
    ],
    "color": "#ff0000"
  },
  "unknownInteraction": {
    "title": "Unbekannte Interaktion",
    "description": "Diese Interaktion ist abgelaufen und steht nicht mehr zur Verfügung",
    "color": "{ $colorError }"
  },
  "permissionsEdit": {
    "title": "Berechtigungen von **{ $target }**",
    "description": "Bitte wähle die Berechtigungen in dem Select-Menu aus:",
    "color": "{ $colorDefault }"
  },
  "permissionsList": {
    "title": "Berechtigungen von **{ $target }**",
    "description": "{ $permissions }",
    "color": "{ $colorDefault }"
  },
  "moderationActExecuted": {
    "title": "{ $type } erfolgreich ausgeführt!",
    "color": "{ $colorSuccess }"
  },
  "moderationTargetBlocked": {
    "title": "Fehler",
    "description": "Der angegebene Nutzer <@{ $targetId }> wird bereits durch <@{ $moderatorId }> moderiert!",
    "color": "{ $colorError }"
  },
  "moderationActTargetInfo": {
    "title": "{ $title }",
    "description": "{ $description }",
    "color": "{ $color }",
    "fields": [
      {
        "name": "Begründung",
        "value": "{ $reason }",
        "inline": true
      },
      {
        "name": "Datum",
        "value": "<t:{ $date }>",
        "inline": true
      },
      {
        "name": "Aufhebung",
        "value": "<t:{ $until }:F> <t:{ $until }:R>"
      },
      {
        "name": "Regel, gegen die du verstoßen hast",
        "value": "{ $paragraph }",
        "inline": false
      },
      {
        "name": "ID",
        "value": "#{ $id }"
      },
      {
        "name": "Referenznachricht",
        "value": "> { $referenceMessage }"
      },
      {
        "name": "Zuständiger Moderator",
        "value": "<@{ $issuerId }> ({ $issuerUsername })"
      }
    ]
  },
  "warnReverted": {
    "title": "Warn aufgehoben",
    "description": "Dein Warn auf dem **NPLAY** Discord Server wurde wieder zurückgenommen!",
